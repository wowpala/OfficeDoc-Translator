# API configuration (get your key from https://cloud.siliconflow.cn/account/ak)
LLM_API_KEY=your_api_key_here

# Translation model (choose from https://cloud.siliconflow.cn/models)
MODEL_NAME=Qwen/Qwen3-8B

# API endpoint
ENDPOINT=https://api.siliconflow.cn/v1

# Temperature (0.0 - 2.0, lower = more deterministic, recommended: 0.7)
TEMPERATURE=0.7

# Enable thinking mode for qwen3 models
ENABLE_THINKING=false
